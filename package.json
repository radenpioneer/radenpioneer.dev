{
  "name": "radenpioneer.dev",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "astro": "astro",
    "build": "pnpm check && astro build",
    "check": "astro sync && pnpm dlx concurrently \"npm:check:*\"",
    "check:lint": "pnpm dlx concurrently \"astro check\" \"tsc --noEmit\"",
    "check:style": "pnpm exec prettier --plugin=prettier-plugin-astro --check .",
    "dev": "astro dev",
    "format": "pnpm exec prettier --plugin=prettier-plugin-astro --write .",
    "preinstall": "npx only-allow pnpm",
    "preview": "astro preview",
    "start": "pnpm build && pnpm preview"
  },
  "dependencies": {
    "@astrojs/check": "^0.2.0",
    "astro": "^3.0.8",
    "astro-seo": "^0.8.0",
    "typescript": "^5.1.6"
  },
  "devDependencies": {
    "postcss": "^8.4.28",
    "postcss-preset-env": "^9.1.3",
    "prettier": "^3.0.2",
    "prettier-plugin-astro": "^0.12.0",
    "sass": "^1.66.1",
    "tailwindcss": "^3.3.3"
  }
}
