{
  "name": "radenpioneer.dev",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "astro": "astro",
    "build": "pnpm check && astro build",
    "check": "astro sync && pnpm dlx concurrently \"npm:check:*\"",
    "check:lint": "pnpm dlx concurrently \"astro check\" \"tsc --noEmit\"",
    "check:style": "prettier --plugin=prettier-plugin-astro --check .",
    "dev": "astro dev",
    "format": "prettier --plugin=prettier-plugin-astro --write .",
    "preinstall": "npx only-allow pnpm",
    "preview": "astro preview",
    "start": "pnpm build && pnpm preview"
  },
  "dependencies": {
    "@astrojs/check": "0.2.0",
    "@astrojs/markdoc": "0.5.0",
    "@astrojs/partytown": "2.0.0",
    "@astrojs/react": "3.0.2",
    "@astrojs/sitemap": "3.0.0",
    "@astrojs/vercel": "4.0.5",
    "@fontsource-variable/urbanist": "5.0.13",
    "@keystatic/astro": "1.0.2",
    "@keystatic/core": "0.1.4",
    "@tailwindcss/typography": "0.5.10",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "astro": "3.0.12",
    "astro-seo": "0.8.0",
    "html-react-parser": "4.2.2",
    "marked": "8.0.1",
    "postcss": "8.4.29",
    "postcss-preset-env": "9.1.3",
    "prettier": "3.0.3",
    "prettier-plugin-astro": "0.12.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sass": "1.66.1",
    "sharp": "0.32.5",
    "tailwindcss": "3.3.3",
    "typescript": "5.2.2"
  }
}
