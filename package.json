{
  "name": "radenpioneer.dev",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "astro": "astro",
    "build": "pnpm check && astro build",
    "check": "astro sync && pnpm dlx concurrently \"npm:check:*\"",
    "check:lint": "pnpm dlx concurrently \"astro check\" \"tsc --noEmit\"",
    "check:style": "pnpm exec prettier --plugin-search-dir=. --check .",
    "format": "pnpm exec prettier --plugin-search-dir=. --write .",
    "dev": "astro dev",
    "preview": "astro preview",
    "start": "astro dev"
  },
  "dependencies": {
    "@astrojs/markdoc": "^0.4.0",
    "@astrojs/react": "^2.2.1",
    "@fontsource-variable/plus-jakarta-sans": "^5.0.5",
    "@fontsource/plus-jakarta-sans": "^5.0.4",
    "@picocss/pico": "^1.5.10",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "astro-seo": "^0.7.5",
    "postcss-preset-env": "^9.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "sass": "^1.63.6",
    "astro": "^2.8.3",
    "typescript": "^5.1.6"
  }
}
