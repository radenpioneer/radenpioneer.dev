{
  "name": "radenpioneer.dev",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "astro": "astro",
    "build": "pnpm check && astro build",
    "check": "astro sync && pnpm dlx concurrently \"npm:check:*\"",
    "check:lint": "pnpm dlx concurrently \"astro check\" \"tsc --noEmit\"",
    "check:style": "prettier --plugin=prettier-plugin-astro --check .",
    "dev": "astro dev",
    "format": "prettier --plugin=prettier-plugin-astro --write .",
    "preinstall": "npx only-allow pnpm",
    "preview": "astro preview",
    "start": "pnpm build && pnpm preview"
  },
  "dependencies": {
    "@astrojs/check": "0.3.1",
    "@astrojs/markdoc": "0.7.1",
    "@astrojs/partytown": "2.0.2",
    "@astrojs/react": "3.0.4",
    "@astrojs/sitemap": "3.0.3",
    "@astrojs/vercel": "5.1.0",
    "@fontsource-variable/urbanist": "5.0.18",
    "@keystatic/astro": "1.0.2",
    "@keystatic/core": "0.2.0",
    "@tailwindcss/typography": "0.5.10",
    "@types/react": "18.2.35",
    "@types/react-dom": "18.2.14",
    "astro": "3.4.3",
    "astro-seo": "0.8.0",
    "html-react-parser": "5.0.6",
    "marked": "10.0.0",
    "postcss": "8.4.31",
    "postcss-preset-env": "9.3.0",
    "prettier": "3.0.3",
    "prettier-plugin-astro": "0.12.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sass": "1.69.5",
    "sharp": "0.32.6",
    "tailwindcss": "3.3.5",
    "typescript": "5.2.2"
  }
}
