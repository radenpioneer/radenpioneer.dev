{
  "name": "radenpioneer.dev",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "astro": "astro",
    "build": "pnpm check && astro build",
    "check": "astro sync && pnpm dlx concurrently \"npm:check:*\"",
    "check:lint": "pnpm dlx concurrently \"astro check\" \"tsc --noEmit\"",
    "check:style": "prettier --plugin=prettier-plugin-astro --check .",
    "codegen": "graphql-codegen --config codegen.ts",
    "dev": "astro dev",
    "format": "prettier --plugin=prettier-plugin-astro --write .",
    "preinstall": "npx only-allow pnpm",
    "preview": "astro preview",
    "start": "pnpm build && pnpm preview"
  },
  "dependencies": {
    "@astrojs/check": "0.3.4",
    "@astrojs/markdoc": "0.8.2",
    "@astrojs/react": "3.0.9",
    "@astrojs/sitemap": "3.0.4",
    "@astrojs/vercel": "6.1.1",
    "@fontsource-variable/urbanist": "5.0.19",
    "@tailwindcss/typography": "0.5.10",
    "@types/react": "18.2.45",
    "@types/react-dom": "18.2.18",
    "astro": "4.0.8",
    "astro-seo": "0.8.0",
    "graphql": "16.8.1",
    "graphql-request": "6.1.0",
    "postcss": "8.4.32",
    "postcss-preset-env": "9.3.0",
    "prettier": "3.1.1",
    "prettier-plugin-astro": "0.12.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sass": "1.69.5",
    "sharp": "0.33.1",
    "tailwindcss": "3.4.0",
    "typescript": "5.3.3"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "5.0.0",
    "@graphql-codegen/client-preset": "4.1.0",
    "@graphql-codegen/typescript": "4.0.1"
  }
}
